{
  "name": "quick-repo",
  "version": "1.0.0",
  "description": "quick repo",
  "main": "build/index.js",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf build/*",
    "tsc": "./node_modules/.bin/tsc",
    "build": "./node_modules/.bin/npm-run-all clean build",
    "start": "./node_modules/.bin/ts-node index.ts",
    "dev": "./node_modules/.bin/nodemon -e ts -V index.ts --exec npm run start",
    "lint": "./node_modules/.bin/eslint ./ --ext .js",
    "test": "./node_modules/.bin/jest --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumeet-bansal/quick-repo.git"
  },
  "author": "Sumeet Bansal",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/sumeet-bansal/quick-repo/issues"
  },
  "homepage": "https://github.com/sumeet-bansal/quick-repo#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  }
}
